#include <civic.h>

void scanVect(int[n] ints) {
	for (int i = 0, n) {
		ints[i] = scanInt();
	}
}

export int main() {
	// This is weird!!! Without the definition of n the code doesn't compile!!!
	// int[3] = [0, 1, 2]; for example fails1?!?!?!?
	int n = 3;
	int[n] ints;
	scanVect(ints);
	for (int i = 0, n) {
		printInt(ints[i]);
	}
	return 0;
}

