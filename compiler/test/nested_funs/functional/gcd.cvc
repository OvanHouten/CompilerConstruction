extern void printInt(int val);
extern void printFloat(float val);

extern int scanInt();
extern float scanFloat();

extern void printSpaces(int num);
extern void printNewlines(int num);

int gcd(int a, int b) {
    // The GCD
    int gcd;

    void swap() {
        int temp = a;
        a = b;
        b = temp;
    }

    // Fix any input error
    if (a < b)
        swap();

    // Euclid method
    if (a % b == 0) {
        gcd = b;
    } else {
        gcd = gcd(b, a % b);
    }

    return gcd;
}

export int main() {
    // Prints the expected GCD and the calculates GCD separated by a space

    printInt(6);
    printSpaces(1);
    printInt(gcd(54, 24));
    printNewlines(2);

    printInt(6);
    printSpaces(1);
    printInt(gcd(24, 54));
    printNewlines(2);

    printInt(1);
    printSpaces(1);
    printInt(gcd(13, 125));
    printNewlines(2);

    printInt(4);
    printSpaces(1);
    printInt(gcd(8, 12));
    printNewlines(2);

    return 0;
}

